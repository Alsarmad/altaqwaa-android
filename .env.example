# Configuration for keystore and certificate
# These values are used to secure the keystore and define the certificate details

STORE_PASSWORD=myStorePassword123
KEY_PASSWORD=myKeyPassword456
ALIAS=myKeyAlias
DNAME_CN=John Doe
DNAME_OU=Development
DNAME_O=MyCompany
DNAME_L=New York
DNAME_S=New York
DNAME_C=US

# Database Configuration
DB_HOST=localhost
DB_PORT=27017
DB_USER=your_db_user
DB_PASS=your_db_password

# Environment setting | (development - production)
NODE_ENV=development

# إيقاف خرائط المصادر
GENERATE_SOURCEMAP=false

# API URL
REACT_APP_API_URL=http://localhost:5000

# AdMob Key
REACT_APP_ADMOB_KEY=your_admob_key_here